<div class="container">
<% if @user %>
<%= render "header"%>

<%= render "nav"%>
<div>
	<%= flash[:notice] %>
</div>
<div class="attendence">
	<div>
		<%= button_to "CHECK IN", attendences_path, method: :post, class:'btn primary' %>
	</div>
	<div>
		<%= button_to "CHECK OUT", attendence_path(@user), method: :patch, class:'btn primary'%>
	</div>
</div>
<% end %>
</div>